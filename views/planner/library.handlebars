<h4>Music Library</h4>
<div id="buttons">
    <a href="/add/library"><button class="btn" id="add_library">Add New Song</button></a>
  </div>

<div class="">
    <div class="col s12">
      <div class="">
        <div class="input-field col s12">
          <i class="material-icons prefix">search</i>
          <input type="text" id="autocomplete-input" class="autocomplete">
          <label for="autocomplete-input">Search Music Library</label>
        </div>
      </div>
    </div>
  </div>

<div class="row">
 <form action="#">
    
      <label for="query_title">
        <input name="query" id="query_title" type="radio" checked />
        <span>Title</span>
      </label>
   
      <label for="query_composer">
        <input name="query" id="query_composer" type="radio" />
        <span>Composer</span>
      </label>
   
      <label for="query_other">
        <input class="" id="query_other" name="query" type="radio"  />
        <span>Other?</span>
      </label>
   
  </form>

</div>


<ul class="collapsible popout">
    {{#each data}}
        {{#if render}}
    <li>
      <div class="collapsible-header" data-id="{{ _id }}"><i class="material-icons">queue_music</i>{{ title }}    
      <br /> by {{ composer }}</div>
      <div class="collapsible-body"><span>{{ refrain }}</span>
        {{#each verses }}
        <p>{{ this }}</p>
        {{/each }}
        <p>{{ copyright }}</p>
        <a href="/library/{{ _id}}" target="_blank" class="waves-effect waves-teal btn-flat edit" data-id="{{ _id }}">Edit</a>
        </div>
    </li>
        {{/if}}
    {{/each}}
</ul>

