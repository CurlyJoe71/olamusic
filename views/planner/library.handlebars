<h4>Music Library</h4>

<div class="row">
    <div class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <i class="material-icons prefix">search</i>
          <input type="text" id="autocomplete-input" class="autocomplete">
          <label for="autocomplete-input">Search Music Library</label>
        </div>
      </div>
    </div>
  </div>

<ul class="collapsible popout">
    {{#each data}}
        {{#if render}}
    <li>
      <div class="collapsible-header" data-id="{{ _id }}"><i class="material-icons">queue_music</i>{{ title }}    
      <br /> by {{ composer }}</div>
      <div class="collapsible-body"><span>{{ refrain }}</span>
        {{#each verses }}
        <p>{{ this }}</p>
        {{/each }}
        <p>{{ copyright }}</p>
        <a href="/library/{{ _id}}" class="waves-effect waves-teal btn-flat edit" data-id="{{ _id }}">Edit</a>
        </div>
    </li>
        {{/if}}
    {{/each}}
</ul>

<div class="row">
    <div class="col s12 m9 l10">
    <div class="col hide-on-small-only m3 l2">
      <ul class="section table-of-contents">
        <li><a href="#">Introduction</a></li>
        <li><a href="#">Structure</a></li>
        <li><a href="#">Intialization</a></li>
      </ul>
    </div>
    </div>
    </div>